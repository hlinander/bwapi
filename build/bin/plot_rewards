set terminal png size 900,900
set output 'reward_plot_unit.png'

set linetype  1 lc rgb "dark-violet" lw 2
set linetype  2 lc rgb "#009e73" lw 2
set linetype  3 lc rgb "#56b4e9" lw 2
set linetype  4 lc rgb "#e69f00" lw 2
set linetype  5 lc rgb "#f0e442" lw 2
set linetype  6 lc rgb "#0072b2" lw 2
set linetype  7 lc rgb "#e51e10" lw 2
set linetype  8 lc rgb "black"   lw 2
set linetype  9 lc rgb "gray50"  lw 2
set linetype cycle  9

plot 'models/model1_rewards' using 1:2 with lines lt 1 title "u1",\
     'models/model2_rewards' using 1:2 with lines lt 3 title "u2",\
     'models/model3_rewards' using 1:2 with lines lt 5 title "u3"

set output 'reward_plot_build.png'

plot 'models/model1_rewards' using 1:3 with lines lt 2 title "b1",\
     'models/model2_rewards' using 1:3 with lines lt 4 title "b2",\
     'models/model3_rewards' using 1:3 with lines lt 6 title "b3"
